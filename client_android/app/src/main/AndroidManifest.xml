<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="moe.retro.unijoysticle.unijosyticle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="moe.retro.unijoysticle.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="moe.retro.unijoysticle.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name="moe.retro.unijoysticle.LinearActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="moe.retro.unijoysticle.UnijoysticleActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="moe.retro.unijoysticle.GyrussActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="moe.retro.unijoysticle.DpadActivity"
            android:screenOrientation="landscape" />
    </application>

</manifest>